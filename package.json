{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint && tsdx build"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "yext-answers-react",
  "author": "Max Shaw",
  "module": "dist/yext-answers-react.esm.js",
  "size-limit": [
    {
      "path": "dist/yext-answers-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/yext-answers-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.3.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@yext/answers-core": "^1.0.0-beta.0",
    "query-string": "^6.13.7",
    "recent-searches": "^1.0.5",
    "use-query-params": "^1.1.9"
  }
}
